{
  "name": "quick-browser",
  "version": "1.0.0",
  "author":"Ninja77030801",
  "description":"A cool browser which helps users enjoy a a desktop-like experience with various websites.",
  "private": true,
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:electron":"electron-builder --win",
    "lint": "vue-cli-service lint"
  },
  "build": {
    "appId":"com.quick.browser",
    "win": {
      "icon":"public/favicon.ico"
    }
  },
  "dependencies": {
    "core-js": "^3.8.3",
    "vue": "^3.2.13"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "electron": "^19.0.7",
    "electron-builder": "^23.1.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
